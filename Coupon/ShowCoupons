var day = newDate.getUTCDate();
var year = newDate.getUTCFullYear();

 newdate = month + "/" + day + "/" + year;



var coupons = document.getElementsByClassName("expireDate");
for (var i = 0; i < coupons.length; i++) {
  coupons[i].innerText = newdate;

